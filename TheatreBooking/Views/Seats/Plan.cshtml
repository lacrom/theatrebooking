<div style="position: absolute; top: 800px; left: 50px;">
    <div class="seat"></div><span style="position: relative; bottom: 15px; left: 20px;"> Свободно</span>
    <div class="seat selected-by-me"></div><span style="position: relative; bottom: 15px; left: 20px;"> Выбрано вами</span>
    <div class="seat selected"></div><span style="position: relative; bottom: 15px; left: 20px;"> Выбрано другими</span>
    <div class="seat booked"></div><span style="position: relative; bottom: 15px; left: 20px;"> Забронировано</span>
</div>
<div class="scene" style="position: absolute; top: 30px; left: 300px; background-color: #d0d0d0; width: 385px; height:60px;"></div>
<div class="input-area">
        <form name="form" novalidate="">
            <div class="my-label">
                <label for="firstName">Имя</label>
            </div>
            <div class="my-input">
                <input id="firstName" type="text" placeholder="Имя" ng-model="firstName" name="first" required/><span class="txt-danger star"> *</span>
            </div>
            <p ng-show="form.first.$dirty && form.first.$invalid" class="txt-danger" ng-cloak>* Поле является обязательным для заполнения</p>
            <div class="my-label">
                <label for="lastName">Фамилия</label>
            </div>
            <div class="my-input">
                <input id="lastName" type="text" placeholder="Фамилия" ng-model="lastName" name="last" required/><span class="txt-danger star"> *</span>
            </div>
            <p ng-show="form.last.$dirty && form.last.$invalid" class="txt-danger" ng-cloak>* Поле является обязательным для заполнения</p>
            <div class="my-label">
                <label for="Email">Email</label>
            </div>
            <div class="my-input">
                <input id="Email" type="email" placeholder="Email" ng-model="email" name="email"/>
            </div>
            <div class="my-label">
                <label for="PhoneNumber">Телефон</label>
            </div>
            <div class="my-input">
                <input id="PhoneNumber" type="tel" placeholder="Телефон" ng-model="phoneNumber" name="phone"/>
            </div>
            <div>
                <button class="book" ng-click="book()" ng-disabled="form.$invalid">
                    Забронировать
                </button>
            </div>
			<div style="margin-top:20px;" ng-cloak>
				<div ng-show="mySelection.length > 0">
					<strong>Вы выбрали:</strong>
				</div>
				<div ng-repeat="(id, seat) in mySelection | filter:{ ID: '! undefined' }" >
					{{seat.AreaDescription}} {{seat.RowName}} {{seat.RowNumber}} Место {{seat.SeatNumber}}
				</div>
			</div>
        </form>
    </div>
</div>
