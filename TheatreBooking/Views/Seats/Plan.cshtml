<div class="input-area">
        <form name="form" novalidate="">
            <div class="my-label">
                <label for="firstName">Имя</label>
            </div>
            <div class="my-input">
                <input id="firstName" type="text" placeholder="Имя" ng-model="firstName" name="first" required/><span class="txt-danger star"> *</span>
            </div>
            <p ng-show="form.first.$dirty && form.first.$invalid" class="txt-danger">* Поле является обязательным для заполнения</p>
            <div class="my-label">
                <label for="lastName">Фамилия</label>
            </div>
            <div class="my-input">
                <input id="lastName" type="text" placeholder="Фамилия" ng-model="lastName" name="last" required/><span class="txt-danger star"> *</span>
            </div>
            <p ng-show="form.last.$dirty && form.last.$invalid" class="txt-danger">* Поле является обязательным для заполнения</p>
            <div class="my-label">
                <label for="Email">Email</label>
            </div>
            <div class="my-input">
                <input id="Email" type="email" placeholder="Email" ng-model="email" name="email" required/><span class="txt-danger star"> *</span>
            </div>
            <p ng-show="form.email.$dirty && form.email.$invalid" class="txt-danger">* Поле является обязательным для заполнения</p>
            <div class="my-label">
                <label for="PhoneNumber">Телефон</label>
            </div>
            <div class="my-input">
                <input id="PhoneNumber" type="tel" placeholder="Телефон" ng-model="phoneNumber" name="phone" required/><span class="txt-danger star"> *</span>
            </div>
            <p ng-show="form.phone.$dirty && form.phone.$invalid " class="txt-danger">* Поле является обязательным для заполнения</p>
            <div ng-repeat="(id, seat) in mySelection | filter:{ ID: '! undefined' }">
                {{seat.AreaDescription}} {{seat.RowName}} {{seat.RowNumber}} Место {{seat.SeatNumber}}
            </div>
            <div>
                <button class="book" ng-click="book()" ng-disabled="form.$invalid">
                    Забронировать
                </button>
            </div>
        </form>
    </div>
